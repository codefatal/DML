<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="food">

 <select id="selectList" resultType="FpFoodVo">
		select * from food
	</select>
	
	<select id="selectOne" resultType="FpFoodVo" parameterType="string">
	select * from food where foodCd=#{foodCd}
	</select>
	
	<select id="mealCodeCheck" resultType="FpFoodVo" parameterType="string">
		select * from food where meal_code=#{mealCode}
	</select>
	
	<insert id="insert" parameterType="FpFoodVo">
		insert into food values(#{foodCd}, #{mealCode}, #{foodQuality})
	</insert>
	
	<update id="update" parameterType="FpFoodVo">
		update food set food_cd=#{foodCd}, food_quality=#{foodQuality}
		where meal_code = #{mealCode}
	</update>
	
	<delete id="delete" parameterType="string">
	delete from food where foodCd = #{foodCd}
	</delete>
</mapper>

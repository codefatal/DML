<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="food">

 <select id="selectList" resultType="FpFoodVo">
		select * from food
	</select>
	
	<select id="selectOne" resultType="FpFoodVo" parameterType="_int">
	select * from  where foodCd=#{foodCd}
	</select>
	
	<insert id="insert" parameterType="FpFoodVo">
	insert into food values(#{foodCd}, #{foodQuality},#{mealKey})
	</insert>
	
	<update id="update" parameterType="FpFoodVo">
	update diet set foodQuality = #{foodQuality},mealKey = #{mealKey}
	where foodCd = #{foodCd}
	</update>
	
	<delete id="delete" parameterType="_int">
	delete from food where mealCode = #{foodCd}
	</delete>
</mapper>
